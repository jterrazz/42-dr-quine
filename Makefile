# **************************************************************************** #
# VARIABLES         														   #
# **************************************************************************** #

NAME = dr_quine

# **************************************************************************** #
# COMMANDS  		    													   #
# **************************************************************************** #

.PHONY: all clean fclean re

all: $(NAME)

$(NAME):
	@make -s -C ./C
	@make -s -C ./ASM

clean:
	@make clean -s -C ./C
	@make clean -s -C ./ASM

fclean:
	@make fclean -s -C ./C
	@make fclean -s -C ./ASM

re:
	@make re -s -C ./C
	@make re -s -C ./ASM
